    <block type="verse">
      <row important_over="false"><ch a="d"/>Hej umarł Stormy dzielny chwat<ch a="A7"/><ch a="d"/></row>
      <row important_over="false"><ch a="g"/>Way hej mister Stormalong<ch a="B"/><ch a="A7"/></row>
      <row important_over="false"><ch a="d"/>Wielki kapitan z dawnych lat<ch a="A7"/><ch a="d"/></row>
      <row important_over="false"><ch a="B"/>Hej mister Stormalong<ch a="A"/><ch a="d"/></row>
