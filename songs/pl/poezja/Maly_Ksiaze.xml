<?xml version="1.0" encoding="utf-8"?>
<song xmlns="http://21wdh.staszic.waw.pl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://21wdh.staszic.waw.pl https://spiewaj.com/song.xsd" title="Mały Książę">
  <alias/>
  <text_author/>
  <comment/>
  <composer type="solo"/>
  <artist type="solo"/>
  <translator/>
  <original_title/>
  <lyric>
    <block type="verse">
      <row important_over="false"><ch a="C"/>Gdzie jesteś Mały Książę gdzie</row>
      <row important_over="false"><ch a="E7"/>Odszedłeś z mej książeczki kart<ch a="F"/></row>
      <bis>
        <row important_over="false"><ch a="F"/>Czy po pustyni błądzisz znów<ch a="G"/><ch a="C"/><ch a="a"/></row>
        <row important_over="false"><ch a="d"/>Rozmawiasz z echem pośród skał<ch a="G"/><ch a="C"/><ch a="C7"/></row>
      </bis>
    </block>
    <block type="verse">
      <row important_over="false"><ch a="C"/>Czy tam gdzie świecie złoty wóz</row>
      <row important_over="false"><ch a="E7"/>Oglądasz Ziemię w swoich snach<ch a="F"/></row>
      <row important_over="false"><ch a="F"/>Czy pozostała z dawnych dni<ch a="G"/><ch a="C"/><ch a="a"/></row>
      <row important_over="false"><ch a="d"/>Niby wspomnienie bajka ta<ch a="G"/><ch a="C"/><ch a="C7"/></row>
    </block>
    <block type="verse">
      <row important_over="false"><ch a="C"/>W maleńkiej róży kochał się</row>
      <row important_over="false"><ch a="E7"/>Książę na jednej wielu gwiazd<ch a="F"/></row>
      <row important_over="false"><ch a="F"/>Nie widział jeszcze innych róż<ch a="G"/><ch a="C"/><ch a="a"/></row>
      <row important_over="false"><ch a="d"/>Kiedy w daleki poszedł świat<ch a="G"/><ch a="C"/><ch a="C7"/></row>
    </block>
    <block type="verse">
      <row important_over="false"><ch a="C"/>Na Ziemi zwątpił w miłość swą</row>
      <row important_over="false"><ch a="E7"/>Ten najpiękniejszy z wszystkich snów<ch a="F"/></row>
      <row important_over="false"><ch a="F"/>Bo jak miał w jednej kochać się<ch a="G"/><ch a="C"/><ch a="a"/></row>
      <row important_over="false"><ch a="d"/>Gdy ujrzał park z tysiącem róż<ch a="G"/><ch a="C"/><ch a="C7"/></row>
    </block>
    <block type="verse">
      <row important_over="false">Nie wierz swym oczom szepną wiatr</row>
      <row important_over="false">Jeżeli kochasz sercem patrz</row>
    </block>
    <block type="verse">
      <row important_over="false"><ch a="C"/>Zrozumiał wtedy książe to</row>
      <row important_over="false"><ch a="E7"/>Że tylko jedna w świecie jest<ch a="F"/></row>
      <row important_over="false"><ch a="F"/>Ta którą kochał z wszystkich sił<ch a="G"/><ch a="C"/><ch a="a"/></row>
      <row important_over="false"><ch a="d"/>I wrócił znów do róży swej<ch a="G"/><ch a="C"/><ch a="C7"/></row>
    </block>
    <block type="verse">
      <row important_over="false"><ch a="C"/>Gdzie jesteś Mały Książe gdzie</row>
      <row important_over="false"><ch a="E7"/>Odszedłeś z mej książeczki kart<ch a="F"/></row>
      <row important_over="false"><ch a="F"/>W świecie gdzie nikt nie kocha róż<ch a="G"/><ch a="C"/><ch a="a"/></row>
      <row important_over="false"><ch a="d"/>Na zawsze ktoś pozostał sam<ch a="G"/><ch a="C"/><ch a="C7"/></row>
    </block>
  </lyric>
  <music metre="">
    <guitar barre="3"/>
  </music>
  <status>
    <verificators/>
    <to_do/>
  </status>
</song>
