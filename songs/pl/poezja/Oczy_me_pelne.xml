<song title="Oczy me pełne…" xmlns="http://21wdh.staszic.waw.pl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://21wdh.staszic.waw.pl https://spiewaj.com/song.xsd">
	<alias></alias>
	<text_author type="solo">Leopold Staff</text_author>
