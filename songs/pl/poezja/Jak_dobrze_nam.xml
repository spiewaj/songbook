	<lyric xmlns="http://21wdh.staszic.waw.pl">

		<block type="verse">

			<row important_over="true">Jak dobrze n<ch a="a"/>am zdobywać g<ch a="d"/>óry</row>

			<row important_over="true">I młodą pi<ch a="E7"/>ersią chłonąć wi<ch a="a"/>atr.</row>

			<row important_over="true">Prężnymi st<ch a="a"/>opy d<ch a="A7"/>eptać chm<ch a="d"/>ury</row>

			<row important_over="true">I palce r<ch a="E7"/>anić o szczyt T<ch a="a"/>atr.</row>

		</block>

		<block type="chorus">

			<row important_over="true">Mieć w uszach sz<ch a="a"/>um, strumieni śpi<ch a="d"/>ew,</row>

			<row important_over="true">A w żyłach r<ch a="E7"/>ozognioną kr<ch a="a"/>ew,</row>

			<row important_over="true">Hejże h<ch a="a"/>ej, hejże h<ch a="E"/>a,</row>

			<row important_over="true">Żyjmy wi<ch a="d"/>ęc póki cz<ch a="a"/>as,</row>

			<row important_over="true">Bo kto wi<ch a="a"/>e, b<ch a="A7"/>o kto wi<ch a="d"/>e,</row>

			<row important_over="true">Kiedy zn<ch a="E7"/>owu ujrzę W<ch a="a"/>as.</row>

		</block>

		<block type="verse">

			<row important_over="false">Jak dobrze n<ch a="a"/>am głęboką n<ch a="d"/>ocą</row>

			<row important_over="false">Wędrować j<ch a="E7"/>asną wstęgą sz<ch a="a"/>os,</row>

			<row important_over="false">Patrzeć jak gwi<ch a="a"/>azdy ni<ch a="A7"/>ebo zł<ch a="d"/>ocą</row>

			<row important_over="false">I czekać, c<ch a="E"/>o przyniesie l<ch a="a"/>os.</row>

		</block>

		<blocklink blocknb="2"/>

		<block type="verse">

			<row important_over="false">Jak dobrze n<ch a="a"/>am tak przy ogn<ch a="d"/>isku</row>

			<row important_over="false">Tęczową wst<ch a="E7"/>ęgę marzeń sn<ch a="a"/>uć,</row>

			<row important_over="false">Patrzeć jak w ni<ch a="a"/>ebo <ch a="A7"/>iskra tr<ch a="d"/>yska</row>

			<row important_over="false">I wokół si<ch a="E7"/>ebe przyjaźń cz<ch a="a"/>uć.</row>

		</block>

		<block type="chorus">

			<row important_over="false">Mieć w oczach bl<ch a="a"/>ask i ogna ż<ch a="d"/>ar,</row>

			<row important_over="false">A w duszy mi<ch a="E7"/>eć młodości cz<ch a="a"/>ar.</row>

			<row important_over="false">Hejże h<ch a="a"/>ej, hejże h<ch a="E"/>a,</row>

			<row important_over="false">Żyjmy wi<ch a="d"/>ęc póki cz<ch a="a"/>as,</row>

			<row important_over="false">Bo kto wi<ch a="a"/>e, b<ch a="A7"/>o kto wi<ch a="d"/>e,</row>

			<row important_over="false">Kiedy zn<ch a="E7"/>owu ujrzę W<ch a="a"/>as.</row>

		</block>

	</lyric>

