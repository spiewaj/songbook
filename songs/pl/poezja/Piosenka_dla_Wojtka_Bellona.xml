	<lyric>
		<block type="verse">
			<row important_over="true">P<ch a="D"/>owiedz d<ch a="G"/>okąd znów wędr<ch a="D"/>ujesz</row>
			<row important_over="true">Cz<ch a="D"/>y dal<ch a="G"/>eko jest twój s<ch a="D"/>ad</row>
			<row important_over="true">Hen w kra<ch a="C"/>iny b<ch a="G"/>uczyn<ch a="D"/>owe</row>
			<row important_over="true">Ze mną t<ch a="C"/>am ukł<ch a="G"/>ada pieśni wi<ch a="D"/>atr</row>
			<row important_over="true">Hen w kra<ch a="e"/>iny buczyn<ch a="D"/>owe</row>
			<row important_over="true">Ze mną t<ch a="e"/>am nik<ch a="G"/>ogo tylko wi<ch a="D"/>atr</row>
		</block>
		<block type="verse">
			<row important_over="false">Zmi<ch a="D"/>erzchy gr<ch a="G"/>ają a przestrz<ch a="D"/>enie</row>
			<row important_over="false">Wł<ch a="D"/>asny <ch a="G"/>im podają dźwi<ch a="D"/>ęk</row>
			<row important_over="false">Takie śpi<ch a="C"/>ewy z n<ch a="G"/>imi lub milcz<ch a="D"/>enie</row>
			<row important_over="false">W którym zn<ch a="C"/>ika k<ch a="G"/>ażdy dawny l<ch a="D"/>ęk</row>
			<row important_over="false">W takich śpi<ch a="e"/>ewach i milcz<ch a="D"/>eniu</row>
			<row important_over="false">W szumie świ<ch a="e"/>ętych b<ch a="G"/>uków zginął l<ch a="D"/>ęk</row>
		</block>
		<block type="verse">
			<row important_over="false">Z<ch a="D"/>aszumi<ch a="G"/>ały cię powi<ch a="D"/>etrza</row>
			<row important_over="false">
				<ch a="D"/>I rusz<ch a="G"/>yłeś sam na szl<ch a="D"/>ak</row>
			<row important_over="false">Ten ost<ch a="C"/>atni t<ch a="G"/>en najl<ch a="D"/>epszy</row>
			<row important_over="false">Przyszedł cz<ch a="C"/>as P<ch a="G"/>an dał ci zn<ch a="D"/>ak</row>
			<bis times="3">
				<row important_over="false">Ten ost<ch a="e"/>atni ten najl<ch a="D"/>epszy</row>
				<row important_over="false">Przyszedł cz<ch a="e"/>as P<ch a="G"/>an dał ci zn<ch a="D"/>ak</row>
